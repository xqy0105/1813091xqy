<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.modules.sys.dao.FmsDocCatalogDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="io.renren.modules.sys.entity.FmsDocCatalogEntity" id="fmsDocCatalogMap">
        <result property="catalogId" column="catalog_id"/>
        <result property="deptId" column="dept_id"/>
        <result property="datalogName" column="datalog_name"/>
    </resultMap>

    <select id="queryByDeptId" parameterType="java.lang.Long" resultType="io.renren.modules.sys.entity.FmsDocCatalogEntity">
        select * from fms_doc_catalog where dept_id = #{deptId, jdbcType=BIGINT}
    </select>

</mapper>